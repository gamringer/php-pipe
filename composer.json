{
    "name": "gamringer/pipe",
    "description": "PSR-15 HTTP Middleware Dispatcher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Amringer",
            "email": "g.amringer@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "psr/http-server-middleware": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "gamringer\\Pipe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gamringer\\Pipe\\Example\\": "example",
            "gamringer\\Pipe\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.4",
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.10.5",
        "phpmd/phpmd": "^2.6",
        "phan/phan": "^1.1"
    }
}
